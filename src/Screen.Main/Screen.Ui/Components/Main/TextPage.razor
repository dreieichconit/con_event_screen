@inherits PageModelPage

<MudStack Class="d-flex flex-grow-1" Justify="Justify.Center" AlignItems="AlignItems.Center">
    @if (Text != null)
    {
        <MudText Typo="Typo.h1">@Text</MudText>
    }
    
    <MudText Typo="Typo.h1">@Page.DisplayText</MudText>
</MudStack>



@code {
    
    [Parameter]
    public string? Text { get; set; }

}